{
    "openapi": "3.0.1",
    "info": {
      "title": "Documment API Hệ thống quản lý công tác khác",
      "description": "des",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "localhost:5151/api"
      }
    ],
    "components": {
      "schemas": {
        "Staff": {
          "type": "object",
          "properties": {
            "_id": {
              "type": "string",
              "example": "6226c32ee6930acc68e2ed8e"
            },
            "staffId": {
              "type": "string",
              "example": 6868
            },
            "name": {
              "type": "string",
              "example": "Le Nhat Phu"
            },
            "email": {
              "type": "string",
              "example": "deptrai@online.com"
            },
            "password": {
              "type": "string",
              "example": "p@ssword"
            }
          }
        }
      }
    },
    "paths": {
      "/auth/register": {
        "post": {
          "tags": [
            "authorization"
          ],
          "summary": "register",
          "operationId": "register",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "staffId": {
                      "type": "string",
                      "example": 6868
                    },
                    "name": {
                      "type": "string",
                      "example": "Le Nhat Phu"
                    },
                    "email": {
                      "type": "string",
                      "example": "deptrai@online.com"
                    },
                    "password": {
                      "type": "string",
                      "example": "p@ssword"
                    }
                  }
                }
              }
            },
            "required": true
          }
        }
      },
      "/auth/login": {
        "post": {
          "tags": [
            "authorization"
          ],
          "summary": "login",
          "operationId": "login",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "staffId": {
                      "type": "string",
                      "example": 6868
                    },
                    "password": {
                      "type": "string",
                      "example": "p@ssword"
                    }
                  }
                }
              }
            },
            "required": true
          }
        }
      }
    }
  }